{
  "name": "toursync",
  "version": "1.0.3",
  "main": "electron/main.js",
  "scripts": {
    "prestart": "npx kill-port 3000",
    "start": "concurrently -k \"cross-env BROWSER=none npm run start:react\" \"wait-on tcp:3000 && npm run start:electron\"",
    "start:react": "vite",
    "start:electron": "electron .",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none npm run start:react\" \"wait-on tcp:3000 && npm run start:electron\"",
    "build": "vite build && electron-builder",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.13.0",
    "@mui/material": "^5.13.0",
    "@mui/x-date-pickers": "^6.5.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.4.7",
    "electron-log": "^5.2.4",
    "electron-updater": "^6.3.9",
    "mongodb": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^8.0.0",
    "cross-env": "^7.0.3",
    "electron": "^24.3.0",
    "electron-builder": "^24.13.3",
    "electron-is-dev": "^2.0.0",
    "vite": "^4.3.0",
    "wait-on": "^7.0.1"
  }
}
